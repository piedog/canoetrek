<!DOCTYPE html>
<html>
<head>
  <title>Map Layout Test</title>
  <%= include_gon %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body class="app">
    <div id="header"><%= render 'layouts/header' %></div>
    <div id="content">
        <div id="middle-container">
            <div id="right-column">Right Column</div>
            <%= yield %>
        </div>
    </div>
    <div id="footer"><%= render 'layouts/footer' %></div>
</body>
</html>
